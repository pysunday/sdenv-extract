{
  "name": "sdenv-extract",
  "version": "0.1.1",
  "description": "动态代码提取与格式化",
  "main": "main.js",
  "bin": {
    "sdenv-extract": "./main.js",
    "sdenv-diff": "./scripts/diff.sh"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pysunday/rs-reverse"
  },
  "keywords": [
    "sdenv",
    "rs-reverse"
  ],
  "files": [
    "package.json",
    "README.md",
    "utils",
    "main.js",
    "src"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "logLevel": "debug",
  "author": "pysunday",
  "license": "BSD 3-Clause",
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "log4js": "^6.9.1",
    "module-alias": "^2.2.3",
    "putout": "^35.7.6",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "sdenv-jsdom": "^1.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^8.0.1",
    "release-it": "^17.0.1"
  },
  "jest": {
    "moduleNameMapper": {
      "@/(.*)": "<rootDir>/$1",
      "@utils/(.*)": "<rootDir>/utils/$1",
      "@src/(.*)": "<rootDir>/src/$1"
    }
  },
  "engines": {
    "node": ">=16"
  },
  "_moduleAliases": {
    "@": ".",
    "@src": "./src",
    "@utils": "./utils"
  }
}
